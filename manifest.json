{
  "manifest_version": 3,
  "name": "KuyPeD Popcat Bot",
  "version": "0.0.9",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "POPCAT Bot"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "96": "icon96.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "activeTab",
    "webNavigation"
  ],
  "host_permissions": [
    "https://popcat.click/"
  ]
}